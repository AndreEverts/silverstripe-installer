{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.3",
        "silverstripe/cms": "3.7.1-rc1",
        "silverstripe/framework": "3.7.1-rc1",
        "silverstripe/reports": "3.7.1-rc1",
        "silverstripe/siteconfig": "3.7.1-rc1",
        "silverstripe-themes/simple": "3.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^3 || ^4 || ^5"
    },
    "extra": [],
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}